{
    "name": "tonybogdanov/magic-services-bundle",
    "description": "Auto-magic service definition generator for Symfony 4.0+ / 5.0+",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tony Bogdanov",
            "email": "support@tonybogdanov.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TonyBogdanov\\MagicServices\\": "src/"
        }
    },
    "require": {
        "php-64bit": "^7.4",
        "ext-json": "*",
        "ext-intl": "*",
        "symfony/framework-bundle": "4.*|5.*",
        "symfony/console": "4.*|5.*",
        "nette/php-generator": "3.*",
        "symfony/filesystem": "4.*|5.*",
        "doctrine/annotations": "1.*",
        "symfony/finder": "4.*|5.*",
        "tonybogdanov/memoize": "^2.0.1",
        "symfony/yaml": "4.*|5.*"
    },
    "require-dev": {
        "symfony/var-dumper": "4.*|5.*"
    }
}
